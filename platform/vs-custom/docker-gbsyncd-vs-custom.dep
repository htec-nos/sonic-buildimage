#DPKG FRK
DPATH       := $($(DOCKER_GBSYNCD_BASE)_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) platform/vs-custom/docker-gbsyncd-vs-custom.mk platform/vs-custom/docker-gbsyncd-vs-custom.dep   
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
