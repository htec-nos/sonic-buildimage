$(DOCKER_GBSYNCD_BASE)_DEP_FILES   := $(DEP_FILES)

$(eval $(call add_dbg_docker,$(DOCKER_GBSYNCD_BASE),$(DOCKER_GBSYNCD_BASE_DBG)))
