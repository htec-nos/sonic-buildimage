SPATH       := $($(DUMMY_HTTP_PKG)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/dummy-http-server.mk rules/dummy-http-server.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(DUMMY_HTTP_PKG)_CACHE_MODE  := GIT_CONTENT_SHA
$(DUMMY_HTTP_PKG)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(DUMMY_HTTP_PKG)_DEP_FILES   := $(DEP_FILES)
$(DUMMY_HTTP_PKG)_SMDEP_FILES := $(SMDEP_FILES)
$(DUMMY_HTTP_PKG)_SMDEP_PATHS := $(SPATH)